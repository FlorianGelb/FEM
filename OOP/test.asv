T = 1;
L = 1;
n = 100;
dx = L/(n);
nt = 10000;
k = (2*pi/L) * [0:n/2-1 -n/2:-1].';
X = linspace(0, L, n);
t = linspace(0, T, nt);
alpha = 0.01;
u0 = (-4*sin(2*pi/L * X));
u0(1) = 0;
u0(end) = 0;
h = 0*randi([-10000 10000], [n,nt]);
h(:, 1) = 0;
%h = eye(n, nt);


m = main(["FEM"], L, u0, h, T, n, nt, alpha);
m = m.solve();
m = m.rom(0.95, 20);
m = m
m.plot();