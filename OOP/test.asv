T = 1;
L = 1;
n = 100;
dx = L/(n);
nt = 10000;
k = (2*pi/L) * [0:n/2-1 -n/2:-1].';
X = linspace(0, L, n);
t = linspace(0, T, nt);
alpha = 0.1;
u0 = 0*(-4*sin(2*pi/L * X));
h = 0*randi([-1000 1000], [n,nt]);
h(:, 1) = 1000000;
%h = eye(n, nt);


m = main(["FEM"], L, u0, h, T, n, nt, alpha);
m = m.solve();
m = m.rom(0.90, 100);
%m.compare(false);
m.plot();